<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiitrans.base.db.model.TermBeanMapper">

	<select id="SelectByTerm" resultType="com.wiitrans.base.db.model.TermBean"
		parameterType="com.wiitrans.base.db.model.TermBean">
		select * from `term` where pair_id=#{pair_id} and industry_id=#{industry_id}
		and term=#{term}
	</select>

	<insert id="Insert" parameterType="com.wiitrans.base.db.model.TermBean">
		insert into
		`term`(pair_id,industry_id,term,create_time)
		values(#{pair_id},#{industry_id},#{term},unix_timestamp(now()))

	</insert>
</mapper>