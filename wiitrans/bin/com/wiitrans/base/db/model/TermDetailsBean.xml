<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiitrans.base.db.model.TermDetailsBeanMapper">

	<select id="SelectTermDetails" resultType="com.wiitrans.base.db.model.TermDetailsBean"
		parameterType="com.wiitrans.base.db.model.TermDetailsBean">
		select * from term_details where term_id=#{term_id} and
		translator_id=#{translator_id};
	</select>

	<insert id="Insert" parameterType="com.wiitrans.base.db.model.TermDetailsBean">
		insert into
		term_details(term_id,translator_id,`meaning`,`usage`,`remark`,`create_time`)
		values(#{term_id},#{translator_id},#{meaning},#{usage},#{remark},unix_timestamp(now()))

	</insert>
</mapper>