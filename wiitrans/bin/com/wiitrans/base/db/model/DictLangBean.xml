<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiitrans.base.db.model.DictLangBeanMapper">
	<select id="SelectAll" resultType="com.wiitrans.base.db.model.DictLangBean">
		select * from dict_language
		order by language_id asc
	</select>
	<select id="Select" resultType="com.wiitrans.base.db.model.DictLangBean">
		select * from dict_language
		where language_id = #{language_id}

	</select>
</mapper>