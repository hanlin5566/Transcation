<sync-datas>
	<sync-data name="updateOrderActualDeliveryTime" >
		<targets>
			<target>
				UPDATE `node_order`
				SET `actual_delivery_time` = ${actual_delivery_time}
				WHERE `order_id` = ${order_id}
			</target>			
		</targets>
		<source>
			SELECT `order_id`,`actual_delivery_time`,#{nid} as 'node_id'
			FROM `order`
			WHERE `order_id` = #{order_id}
		</source>
	</sync-data>
