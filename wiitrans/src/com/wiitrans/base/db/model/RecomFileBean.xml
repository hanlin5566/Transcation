<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiitrans.base.db.model.RecomFileBeanMapper">

	<select id="SelectFiles" resultType="com.wiitrans.base.db.model.RecomFileBean">

		SELECT file_id,order_id,name,word_count,analyse_word_count,preview
		FROM `order_file` WHERE order_id = #{order_id};


	</select>
	
	<select id="Select" resultType="com.wiitrans.base.db.model.RecomFileBean">

		SELECT file_id,order_id,name,word_count,analyse_word_count,preview
		FROM `order_file` WHERE file_id = #{file_id};


	</select>
</mapper>